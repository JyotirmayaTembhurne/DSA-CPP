class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int counter = count(nums.begin(), nums.end(), 0);
        auto newend = remove(nums.begin(), nums.end(), 0);
        nums.erase(newend, nums.end());
        while (counter != 0){
            nums.push_back(0);
            counter--;
        }
    }
};
