#include <set>
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        std :: set<int> uniqueNums(nums.begin(), nums.end());
        nums.clear();
        std :: copy(uniqueNums.begin(), uniqueNums.end(), std :: back_inserter(nums));
        return uniqueNums.size();
    }
};
